{
  "name": "casino",
  "version": "1.0.0",
  "description": "Casino application with clean architecture",
  "main": "dist/web-server.js",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc",
    "build:client": "tsc --project client/tsconfig.json --noEmit && vite build",
    "start": "node dist/web-server.js",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "ts-node src/web-server.ts",
    "dev:client": "vite dev",
    "watch": "tsc --watch"
  },
  "keywords": [
    "casino",
    "typescript",
    "games"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^7.2.2"
  }
}
